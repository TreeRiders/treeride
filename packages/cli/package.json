    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/ink": "^2.0.3",
    "@types/react": "^18.0.32",
    "chalk": "^5.2.0",
